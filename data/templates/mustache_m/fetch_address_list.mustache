{{#results.rows}}
      <div class="item {{ #is_default_label }}active {{/is_default_label}}" id="{{ _id }}" data-id="{{ _id }}">
        <a class="content" {{ #is_from_order }}href="{{ app_url_wap }}/shop/address_checkout?addrid={{ _id }}&rrid={{ rrid }}"{{ /is_from_order }}>
          <div class="addr">
            <h5 class="ui header pt-r mb-2r">{{ name }}</h5>
            <span>{{ area_province.city}} {{ area_district.city }}</span>
            <span> {{ address }} </span>
            <span>{{ phone }}</span>
          </div>
		  
		  <img class="mr-r" src="http://frstatic.qiniudn.com/images/icon/shipnext.png" width="11px"> 
          
        </a>
        <div class="actions">
          {{ #is_default_label }}
          <img src="{{ results.app_url_packaged }}/images/icon/is_default.png" width="71">
          {{ /is_default_label }}
          <div class="revise">
            <a class="ui small gray icon edit button" data-id="{{ _id }}">
              <i class="edit icon"></i>&nbsp;&nbsp;编辑
            </a>
            <a class="ui small gray icon remove button" data-id="{{ _id }}">
              <i class="remove icon"></i>&nbsp;&nbsp;删除
            </a>
          </div>
        </div>
      </div>
{{/results.rows}}
