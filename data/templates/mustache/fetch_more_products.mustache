{{#results.rows}}<div class="card" id="product-{{ _id }}">    <div class="problock">        <a title="{{ title }}" alt="{{#product_alt_tag}}{{ tags.0 }}-太火鸟智能硬件商店{{/product_alt_tag}}{{^product_alt_tag}}{{ title }}-太火鸟智能硬件商店{{/product_alt_tag}}" href="{{ view_url }}" class="thumb" target="_blank">            <img class="lazy" data-original="{{ cover.thumbnails.apc.view_url }}" src="https://s3.taihuoniao.com/images/icon/loadbg0.jpg">            {{#video}}            <span class="videomark">                <i class="icon video play"></i>            </span>            {{/video}}        </a>        <div class="ribbons">            {{#new_tips}}            <a class="ribbon xinpin" href="javascript:void(0);"></a>			<p class="ribbonp">新品</p>            {{/new_tips}}            {{#hot_tips}}            <a class="ribbon remai" href="javascript:void(0);"></a>			<p class="ribbonp">热卖</p>            {{/hot_tips}}        </div>        <h2>            <a href="{{ view_url }}" title="{{ title }}" class="ui line" target="_blank">			    {{ title }}		    </a>        </h2>        <div class="excerpt">            <p>{{ advantage }}</p>        </div>        <div class="cats">        {{^idea}}            {{#hotsale}}            <i class="yen icon"></i>{{ sale_price }}            {{/hotsale}}            {{#presale}}            <i class="yen icon"></i>{{ sale_price }}            {{/presale}}        {{/idea}}        </div>        <span class="counter" style="right:60px;">            <i class="unhide icon"></i>{{ view_count }}        </span>        <span class="counter">            <i class="heart icon"></i> {{ love_count }}        </span>    </div></div>{{/results.rows}}